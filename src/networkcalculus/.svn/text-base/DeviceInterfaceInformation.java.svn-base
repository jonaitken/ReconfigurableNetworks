package networkcalculus;

import java.util.ArrayList;

public class DeviceInterfaceInformation {
	
	String stringDeviceName;
	ArrayList myListInterfaces=new ArrayList();
	
	public DeviceInterfaceInformation(String stringDeviceName) {
		this.stringDeviceName=new String(stringDeviceName);
	}
	
	public String getDeviceName() {
		return this.stringDeviceName;
	}
	
	@SuppressWarnings("unchecked")
	public void addInterfaces(String stringInterface) {
		this.myListInterfaces.add(new String(stringInterface));
	}
	
	public ArrayList getInterfaces() {
		return this.myListInterfaces;
	}
}
